PROC swap(int &x, int &y) {
	
	int tmp = x;
	x = y;
	y = tmp;
	
}

PROC selection_sort(int a[], int N) {
	
	int i, j;
	FOR (i, 0 TO N-2) {
		int minj = i;
		FOR (j, i+1 TO N-1)
			if (a[j] < a[minj])
				minj = j;
		swap(a[i], a[minj]);
	}

}

PROGRAM test() {

	int i, a[10];
	FOR (i, 0 TO 9) 
		a[i] = READ_INT();
	
	selection_sort(a, 10);
	
	FOR (i, 0 TO 9)
		WRITELN(a[i]);
	WRITELN();
	
}
